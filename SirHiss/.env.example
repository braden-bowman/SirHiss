# SirHiss Trading Bot Platform Environment Configuration
# Copy this file to .env and update with your actual values

# Database Configuration
POSTGRES_USER=sirhiss
POSTGRES_PASSWORD=sirhiss_secure_pwd
POSTGRES_DB=sirhiss_db

# Application Security
SECRET_KEY=your-super-secret-key-change-this-in-production
ENVIRONMENT=development

# Robinhood API Credentials
# IMPORTANT: Set these to your actual Robinhood credentials
ROBINHOOD_USERNAME=your_robinhood_username
ROBINHOOD_PASSWORD=your_robinhood_password

# Redis Configuration (usually don't need to change)
REDIS_URL=redis://redis:6379

# API URLs (usually don't need to change for Docker setup)
REACT_APP_API_URL=http://localhost:9002/api/v1
REACT_APP_WS_URL=ws://localhost:9002

# Bot Configuration
MAX_CONCURRENT_BOTS=10
BOT_EXECUTION_INTERVAL=60

# Logging Level
LOG_LEVEL=INFO